.tab-bar {
  border-radius: 20rpx 20rpx 0 0;
  border-top: 1px solid #f6f6f6;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  height: 120rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  z-index: 99;
}
 
.tab-bar-item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
 
.txt{
  font-size: 36rpx;
  color: #aaaaaa;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.font_weight{
  color: #333;
  font-weight: bold;
}
.bg_rec{
  background: #f7a908;
  width: 76rpx;
  height: 20rpx;
  margin-top: -28rpx;
  vertical-align: text-bottom;
  z-index: -1;
}
.center_img{
  margin-bottom: 15rpx;
  width: 100rpx;
  height: 100rpx;
}
