/* components/DatePick/date_pick.wxss */
.info_type{
  border-bottom: 1px solid #e6e6e6;
  width: 100%;
  height: 94rpx;
  padding-left: 15rpx;
  padding-right: 15rpx;
  display: flex;
  align-items: center;
  justify-content: space-between; 
}
.info_left{
  display: flex;
  align-items: center;
}
.info_label{
  width: 150rpx;
  font-size: 32rpx;
  color: #333;
}

.info_text_null{
  font-size: 30rpx;
  color: #8a8a8a;
}
.info_text{
  font-size: 30rpx;
  color: #333;
}
.info_right{
  width: 32rpx;
  height: 32rpx;
}